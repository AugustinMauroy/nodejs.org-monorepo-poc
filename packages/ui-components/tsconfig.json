{
  "compilerOptions": {
    "outDir": "./dist/esm",
    "module": "ESNext",
    "target": "es5",
    "lib": ["es2017", "dom"],
    "jsx": "react",
    "moduleResolution": "node",
    "baseUrl": ".",
    "declaration": true,
    "strict": true,
    "noImplicitAny": true,
    "skipLibCheck": true,
  },
  "include": ["src/**/*.tsx"]
}